<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color323f4b"/>

            <corners
                android:radius="20dp"/>
        </shape>

    </item>
    <item android:state_pressed="false">
        <shape>
            <solid android:color="@color/color252e37"/>

            <corners
                android:radius="20dp"/>
        </shape>
    </item>
</selector>