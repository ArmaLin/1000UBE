<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_dashboard_media"
    app:startDestination="@id/dashboardMediaFragment">

    <fragment
        android:id="@+id/dashboardMediaFragment"
        android:name="com.dyaco.spirit_commercial.dashboard_media.DashboardMediaFragment"
        android:label="fragment_dashboard_media"
        tools:layout="@layout/fragment_dashboard_media" >
        <action
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out"
            android:id="@+id/action_dashboardMediaFragment_to_channelsFragment"
            app:destination="@id/channelsFragment" />
    </fragment>
    <fragment
        android:id="@+id/channelsFragment"
        android:name="com.dyaco.spirit_commercial.dashboard_media.ChannelsFragment"
        android:label="fragment_channel"
        tools:layout="@layout/fragment_channel" />
</navigation>