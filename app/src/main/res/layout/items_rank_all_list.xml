<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/cl_base"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="92dp"
    xmlns:tools="http://schemas.android.com/tools">
    <TextView
        android:fontFamily="@font/inter_regular"
        android:layout_marginStart="48dp"
        android:id="@+id/tvNo"
        android:layout_width="60dp"
        android:textColor="@color/colorADB8C2"
        android:layout_height="wrap_content"
        android:gravity="start|center_vertical"
        tools:text="1"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageView
        tools:src="@drawable/shape_oval_grey"
        android:id="@+id/ivMemberAvatarPhoto"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="261dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="48dp"
        android:layout_height="48dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/ivMemberAvatar"
        app:layout_constraintEnd_toEndOf="@+id/ivMemberAvatarPhoto"
        app:layout_constraintStart_toStartOf="@+id/ivMemberAvatarPhoto"
        app:layout_constraintTop_toTopOf="@+id/ivMemberAvatarPhoto"
        app:layout_constraintBottom_toBottomOf="@+id/ivMemberAvatarPhoto"
        android:layout_width="68dp"
        android:layout_height="68dp" />

    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="32sp"
        android:textColor="@color/color5a7085"
        tools:text="name"
        android:layout_marginStart="333dp"
        android:id="@+id/tvMemberName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="32sp"
        android:textColor="@color/color1396ef"
        tools:text="136.5"
        android:layout_marginEnd="759dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/tvHours"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>


    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="32sp"
        android:textColor="@color/colorADB8C2"
        tools:text="10.3"
        android:layout_marginEnd="522dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/tvSpeed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="32sp"
        android:textColor="@color/colorADB8C2"
        tools:text="20.5"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="285dp"
        android:layout_marginStart="1597dp"
        android:id="@+id/tvDistance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />


    <TextView
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="48dp"
        android:fontFamily="@font/inter_regular"
        android:textSize="32sp"
        android:textColor="@color/colorADB8C2"
        tools:text="15.5"
        android:layout_marginStart="1825dp"
        android:id="@+id/tvCal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
