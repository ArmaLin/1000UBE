<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fl_root"
    android:layout_width="362dp"
    android:background="@drawable/bg_tv_controller"
    android:layout_height="match_parent"
    tools:context=".dashboard_media.TvControllerWindow">
<!--+50-->


    <TextView
        android:fontFamily="@font/inter_bold"
        android:layout_marginStart="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/vLine1"
        tools:text="@string/NBC_Sports"
        android:id="@+id/tvTitle"
        android:textSize="32sp"
        android:textColor="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <TextView
        android:fontFamily="@font/inter_regular"
        android:layout_marginStart="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/vLine2"
        tools:text="@string/snl_the_early_years"
        android:id="@+id/tvTitle2"
        android:textSize="24sp"
        android:textColor="@color/colorADB8C2"
        app:layout_constraintTop_toBottomOf="@+id/vLine1"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btnVolumePlus"
        android:id="@+id/btnVolumePlus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/btn_tv_plus"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vLine2"
        tools:ignore="SpeakableTextPresentCheck" />


    <TextView
        android:gravity="center_vertical|center_horizontal"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/inter_bold"
        app:layout_constraintStart_toStartOf="@+id/btnVolumePlus"
        tools:text="40"
        android:id="@+id/tvVolumeNum"
        android:textSize="44sp"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/btnVolumePlus"
        app:layout_constraintEnd_toEndOf="@+id/btnVolumePlus"
        android:layout_width="wrap_content"
        android:layout_height="64dp" />

    <TextView
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/inter_regular"
        app:layout_constraintEnd_toEndOf="@+id/btnVolumeMinus"
        app:layout_constraintStart_toStartOf="@+id/btnVolumeMinus"
        app:layout_constraintBottom_toTopOf="@+id/btnVolumeMinus"
        android:text="@string/Volume"
        android:textSize="24sp"
        android:textColor="@color/colorADB8C2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btnVolumeMinus"
        android:id="@+id/btnVolumeMinus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/btn_tv_minus"
        app:layout_constraintBottom_toTopOf="@+id/vLine3"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />


    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btnChannelUp"
        android:id="@+id/btnChannelUp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn_tv_up"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vLine2"
        tools:ignore="SpeakableTextPresentCheck" />


    <TextView
        android:gravity="center_vertical|center_horizontal"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/inter_bold"
        app:layout_constraintStart_toStartOf="@+id/btnChannelUp"
        tools:text="40"
        android:id="@+id/tvChannelNum"
        android:textSize="44sp"
        android:textColor="@color/white"
        app:layout_constraintTop_toBottomOf="@+id/btnChannelUp"
        app:layout_constraintEnd_toEndOf="@+id/btnChannelUp"
        android:layout_width="wrap_content"
        android:layout_height="64dp" />

    <TextView
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/inter_regular"
        app:layout_constraintEnd_toEndOf="@+id/btnChannelUp"
        app:layout_constraintStart_toStartOf="@+id/btnChannelUp"
        app:layout_constraintBottom_toTopOf="@+id/btnChannelDown"
        android:text="@string/Channel"
        android:textSize="24sp"
        android:textColor="@color/colorADB8C2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />


    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btnChannelDown"
        android:id="@+id/btnChannelDown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:background="@drawable/btn_tv_down"
        app:layout_constraintBottom_toTopOf="@+id/vLine3"
        app:layout_constraintEnd_toEndOf="@+id/btnChannelUp"
        app:layout_constraintStart_toStartOf="@+id/btnChannelUp"
        tools:ignore="SpeakableTextPresentCheck" />



    <CheckBox
        android:id="@+id/btn_sound_off"
        android:checked="true"
        android:button="@null"
        android:background="@drawable/btn_sound_check"
        app:layout_constraintBottom_toTopOf="@+id/vLine4"
        app:layout_constraintEnd_toEndOf="@+id/btnVolumePlus"
        app:layout_constraintStart_toStartOf="@+id/btnVolumePlus"
        app:layout_constraintTop_toBottomOf="@+id/vLine3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

<!--    <androidx.appcompat.widget.AppCompatImageButton-->
<!--        android:tag="btn_sound_off"-->
<!--        android:id="@+id/btn_sound_off"-->
<!--        android:layout_width="wrap_content"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:background="@drawable/btn_tv_sound_off"-->
<!--        app:layout_constraintBottom_toTopOf="@+id/vLine4"-->
<!--        app:layout_constraintEnd_toEndOf="@+id/btnVolumePlus"-->
<!--        app:layout_constraintStart_toStartOf="@+id/btnVolumePlus"-->
<!--        app:layout_constraintTop_toBottomOf="@+id/vLine3"-->
<!--        tools:ignore="SpeakableTextPresentCheck" />-->

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_sub"
        android:id="@+id/btn_sub"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_sub"
        app:layout_constraintBottom_toTopOf="@+id/vLine4"
        app:layout_constraintEnd_toEndOf="@+id/btnChannelUp"
        app:layout_constraintStart_toStartOf="@+id/btnChannelUp"
        app:layout_constraintTop_toBottomOf="@+id/vLine3"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num1"
        android:id="@+id/btn_num1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/btn_tv_num_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vLine4"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num2"
        android:id="@+id/btn_num2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_2"
        app:layout_constraintBottom_toBottomOf="@+id/btn_num1"
        app:layout_constraintStart_toEndOf="@+id/btn_num1"
        app:layout_constraintTop_toTopOf="@+id/btn_num1"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num3"
        android:id="@+id/btn_num3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_3"
        app:layout_constraintBottom_toBottomOf="@+id/btn_num1"
        app:layout_constraintStart_toEndOf="@+id/btn_num2"
        app:layout_constraintTop_toTopOf="@+id/btn_num1"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num4"
        android:id="@+id/btn_num4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:background="@drawable/btn_tv_num_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_num1"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num5"
        android:id="@+id/btn_num5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_5"
        app:layout_constraintBottom_toBottomOf="@+id/btn_num4"
        app:layout_constraintStart_toEndOf="@+id/btn_num4"
        app:layout_constraintTop_toTopOf="@+id/btn_num4"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num6"
        android:id="@+id/btn_num6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_6"
        app:layout_constraintBottom_toBottomOf="@+id/btn_num4"
        app:layout_constraintStart_toEndOf="@+id/btn_num5"
        app:layout_constraintTop_toTopOf="@+id/btn_num4"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num7"
        android:id="@+id/btn_num7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="74dp"
        android:background="@drawable/btn_tv_num_7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_num4"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num8"
        android:id="@+id/btn_num8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_8"
        app:layout_constraintBottom_toBottomOf="@+id/btn_num7"
        app:layout_constraintStart_toEndOf="@+id/btn_num7"
        app:layout_constraintTop_toTopOf="@+id/btn_num7"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num9"
        android:id="@+id/btn_num9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_9"
        app:layout_constraintBottom_toBottomOf="@+id/btn_num7"
        app:layout_constraintStart_toEndOf="@+id/btn_num8"
        app:layout_constraintTop_toTopOf="@+id/btn_num7"
        tools:ignore="SpeakableTextPresentCheck" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:tag="btn_num0"
        android:id="@+id/btn_num0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btn_tv_num_0"
        app:layout_constraintEnd_toEndOf="@+id/btn_num8"
        app:layout_constraintStart_toStartOf="@+id/btn_num8"
        app:layout_constraintTop_toBottomOf="@+id/btn_num8"
        tools:ignore="SpeakableTextPresentCheck" />

    <View
        android:id="@+id/vLine1"
        android:background="@color/color323f4b"
        android:layout_marginTop="102dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="312dp"
        android:layout_height="2dp"/>

    <View
        android:id="@+id/vLine2"
        android:background="@color/color323f4b"
        android:layout_marginTop="214dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="312dp"
        android:layout_height="2dp"/>

    <View
        android:id="@+id/vLine3"
        android:background="@color/color323f4b"
        android:layout_marginTop="534dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="312dp"
        android:layout_height="2dp"/>

    <View
        android:id="@+id/vLine4"
        android:background="@color/color323f4b"
        android:layout_marginTop="646dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_width="312dp"
        android:layout_height="2dp"/>



