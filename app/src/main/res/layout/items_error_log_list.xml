<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/cl_base"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:clickable="true"
    android:layout_marginBottom="16dp"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">
    <TextView

        android:fontFamily="@font/inter_regular"
        android:id="@+id/tvDate"
        android:layout_width="140dp"
        android:textColor="@color/color1396ef"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        tools:text="100"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="24sp"
        android:textColor="@color/color0dac87"
        tools:text="name"
        android:layout_marginStart="168dp"
        android:id="@+id/tvTime"
        android:layout_width="260dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="24sp"
        android:textColor="@color/colorCd5bff"
        tools:text="136.5"
        android:layout_marginStart="448dp"
        android:id="@+id/tvErrorCode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:fontFamily="@font/inter_regular"
        android:textSize="24sp"
        android:textColor="@color/colore24b44"
        android:text=""
        android:layout_marginStart="600dp"
        android:id="@+id/tvErrorMessage"
        android:layout_width="473dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
