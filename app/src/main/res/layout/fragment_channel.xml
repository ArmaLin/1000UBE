<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="904dp"
    android:background="@color/color11151a"
    tools:context=".dashboard_media.ChannelsFragment">


    <com.google.android.material.button.MaterialButton
        android:letterSpacing="0.01"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:id="@+id/btnBack"
        android:layout_width="165dp"
        android:layout_height="88dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/panel_bg_all_20_252e37"
        android:fontFamily="@font/inter_bold"
        android:stateListAnimator="@null"
        android:text="@string/back"
        android:textAllCaps="false"
        android:textColor="@color/colorADB8C2"
        android:textSize="24sp"
        app:backgroundTint="@color/color252e37"
        app:backgroundTintMode="src_in"
        app:icon="@drawable/icon_back"
        app:iconGravity="textStart"
        app:iconPadding="12dp"
        app:iconTint="@color/colorADB8C2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:includeFontPadding="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="261dp"
        android:fontFamily="@font/inter_bold"
        android:text="@string/TV"
        android:textColor="@color/white"
        android:textSize="70sp"
        app:layout_constraintBottom_toBottomOf="@+id/btnBack"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnBack" />

    <com.dyaco.spirit_commercial.support.custom_view.RelativeRadioGroup
        android:id="@+id/rgSelectChannel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="@+id/btnBack"
        app:layout_constraintTop_toBottomOf="@id/btnBack">

        <RadioButton
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/rbAll"
            android:layout_width="98dp"
            android:layout_height="88dp"
            android:background="@drawable/btn_nine_select"
            android:button="@null"
            android:checked="true"
            android:fontFamily="@font/inter_bold"
            android:gravity="center"
            android:text="@string/All"
            android:textColor="@drawable/check_text_5a7085"
            android:textSize="32sp" />

        <RadioButton
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/rbNews"
            android:layout_width="144dp"
            android:layout_height="88dp"
            android:layout_marginStart="24dp"
            android:layout_toEndOf="@+id/rbAll"
            android:background="@drawable/btn_nine_select"
            android:button="@null"
            android:fontFamily="@font/inter_bold"
            android:gravity="center"
            android:text="@string/News"
            android:textColor="@drawable/check_text_5a7085"
            android:textSize="32sp" />

        <RadioButton
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/rbMovies"
            android:layout_width="196dp"
            android:layout_height="88dp"
            android:layout_marginStart="24dp"
            android:layout_toEndOf="@+id/rbNews"
            android:background="@drawable/btn_nine_select"
            android:button="@null"
            android:fontFamily="@font/inter_bold"
            android:gravity="center"
            android:text="@string/Movies"
            android:textColor="@drawable/check_text_5a7085"
            android:textSize="32sp" />

        <RadioButton
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/rbMusic"
            android:layout_width="151dp"
            android:layout_height="88dp"
            android:layout_below="@+id/rbAll"
            android:layout_marginTop="24dp"
            android:background="@drawable/btn_nine_select"
            android:button="@null"
            android:fontFamily="@font/inter_bold"
            android:gravity="center"
            android:text="@string/Music"
            android:textColor="@drawable/check_text_5a7085"
            android:textSize="32sp" />

        <RadioButton
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/rbSports"
            android:layout_width="151dp"
            android:layout_height="88dp"
            android:layout_below="@+id/rbAll"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:layout_toEndOf="@+id/rbMusic"
            android:background="@drawable/btn_nine_select"
            android:button="@null"
            android:fontFamily="@font/inter_bold"
            android:gravity="center"
            android:text="@string/Sports"
            android:textColor="@drawable/check_text_5a7085"
            android:textSize="32sp" />

        <RadioButton
            android:foreground="?attr/selectableItemBackgroundBorderless"
            android:id="@+id/rbComedy"
            android:layout_width="151dp"
            android:layout_height="88dp"
            android:layout_below="@+id/rbAll"
            android:layout_marginStart="24dp"
            android:layout_marginTop="24dp"
            android:layout_toEndOf="@+id/rbSports"
            android:background="@drawable/btn_nine_select"
            android:button="@null"
            android:checked="false"
            android:fontFamily="@font/inter_bold"
            android:gravity="center"
            android:text="@string/Comedy"
            android:textColor="@drawable/check_text_5a7085"
            android:textSize="32sp" />

    </com.dyaco.spirit_commercial.support.custom_view.RelativeRadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="428dp"
        android:fontFamily="@font/inter_regular"
        android:text="@string/Input_Number"
        android:textColor="@color/color5a7085"
        android:textSize="44sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="536dp"
        android:layout_marginTop="444dp"
        android:fontFamily="@font/inter_bold"
        android:text="@string/Select"
        android:textColor="@color/color1396ef"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

