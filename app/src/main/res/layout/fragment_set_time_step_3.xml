<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="904dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dashboard_training.SetTimeStep3Fragment">


    <View
        android:background="@drawable/shape_oval_323f4b"
        android:visibility="visible"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="130dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/viewStep1"
        android:layout_width="12dp"
        android:layout_height="12dp" />

    <View
        android:visibility="visible"
        android:background="@drawable/shape_oval_323f4b"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="94dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/viewStep2"
        android:layout_width="12dp"
        android:layout_height="12dp" />

    <View
        android:visibility="visible"
        android:background="@drawable/shape_oval_1396ef"
        android:layout_marginTop="86dp"
        android:layout_marginEnd="58dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:id="@+id/viewStep3"
        android:layout_width="12dp"
        android:layout_height="12dp" />

    <com.google.android.material.button.MaterialButton
        style="@style/MButton_252e37"
        android:id="@+id/btnBack"
        android:paddingStart="40dp"
        android:paddingEnd="40dp"
        android:layout_width="wrap_content"
        android:layout_height="88dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:text="@string/back"
        android:textColor="@color/colorADB8C2"
        android:textSize="24sp"
        app:icon="@drawable/icon_back"
        app:iconGravity="textStart"
        app:iconPadding="12dp"
        app:iconTint="@color/colorADB8C2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        tools:text="@string/hiit"
        android:id="@+id/tvProgramName"
        android:fontFamily="@font/inter_bold"
        android:text="@string/HIIT"
        android:gravity="center_vertical"
        app:layout_constraintBottom_toBottomOf="@+id/btnBack"
        app:layout_constraintTop_toTopOf="@+id/btnBack"
        app:layout_constraintStart_toEndOf="@+id/btnBack"
        android:layout_marginStart="48dp"
        android:textColor="@color/white"
        android:textSize="70sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@drawable/panel_bg_all_20_252e37"
        android:id="@+id/timeView"
        android:layout_marginTop="168dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="0dp"
        android:layout_height="344dp">


        <androidx.appcompat.widget.AppCompatTextView
            android:background="@drawable/color_252e37_323f4b"
            android:id="@+id/tvTimeNum"
            android:layout_width="228dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="48dp"
            android:fontFamily="@font/inter_extrabold"
            android:gravity="center_vertical|center_horizontal"
            android:includeFontPadding="false"
            android:text=""
            android:textColor="@color/white"
            android:textSize="96sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:contentDescription="@string/minus"
            android:id="@+id/btnTimeMinus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="52dp"
            android:background="@drawable/btn_minus"
            app:layout_constraintBottom_toBottomOf="@+id/tvTimeNum"
            app:layout_constraintEnd_toStartOf="@+id/tvTimeNum"
            app:layout_constraintTop_toTopOf="@+id/tvTimeNum" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:contentDescription="@string/plus"
            android:id="@+id/btnTimePlus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="52dp"
            android:background="@drawable/btn_plus"
            app:layout_constraintBottom_toBottomOf="@+id/tvTimeNum"
            app:layout_constraintStart_toEndOf="@+id/tvTimeNum"
            app:layout_constraintTop_toTopOf="@+id/tvTimeNum" />

        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="48dp"
            android:layout_marginTop="78.4dp"
            android:background="@drawable/icon_time_48"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="ImageContrastCheck" />

        <androidx.appcompat.widget.AppCompatTextView
            android:textColor="@color/white"
            android:id="@+id/tvTimeTitle"
            android:text="@string/Intervals"
            android:textSize="44sp"
            android:fontFamily="@font/inter_regular"
            android:layout_marginStart="128.06dp"
            android:layout_marginTop="68dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnTimeMinus"
            android:gravity="center_vertical"
            android:layout_width="0dp"
            android:layout_marginEnd="30dp"
            android:autoSizeTextType="uniform"
            android:maxLines="1"
            android:autoSizeMaxTextSize="44sp"
            android:layout_height="wrap_content" />

        <com.dyaco.spirit_commercial.support.custom_view.RulerView
            android:id="@+id/rulerViewTime"
            android:layout_width="0dp"
            android:layout_height="130dp"
            android:layout_marginStart="36dp"
            android:layout_marginEnd="36dp"
            android:layout_marginBottom="48dp"
            android:background="@color/color252e37"
            android:clipToPadding="false"
            android:paddingStart="52dp"
            android:paddingEnd="52dp"
            app:layout_constraintBottom_toBottomOf="@+id/timeView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:rulerColor="@color/colorADB8C2"
            app:rulerDivideByFiveHeight="40dp"
            app:rulerDivideByFiveWidth="3dp"
            app:rulerDivideByTenHeight="40dp"
            app:rulerDivideByTenWidth="3dp"
            app:rulerHighlightColor="@color/white"
            app:rulerIntervalDistance="40dp"
            app:rulerIntervalValue="1"
            app:rulerIsDivideByFive="true"
            app:rulerIsDivideByTen="true"
            app:rulerLineHeight="28dp"
            app:rulerLineWidth="3dp"
            app:rulerMaxValue="15"
            app:rulerMinValue="3"
            app:rulerOrientation2="horizontal"
            app:rulerRetainLength="0"
            app:rulerTextBaseLineDistance="16dp"
            app:rulerTextColor="@color/color5a7085"
            app:rulerTextSize="24sp" />

        <!--    小數點-->
        <!--    app:rulerIntervalValue="0.1"-->
        <!--    app:rulerRetainLength="1"-->

        <View
            android:layout_marginBottom="12dp"
            android:id="@+id/rulerViewTimeCenter1"
            android:layout_width="6dp"
            android:layout_height="64dp"
            android:background="@color/colore24b44"
            app:layout_constraintBottom_toBottomOf="@+id/rulerViewTime"
            app:layout_constraintEnd_toEndOf="@+id/rulerViewTime"
            app:layout_constraintStart_toStartOf="@+id/rulerViewTime"
            app:layout_constraintTop_toTopOf="@+id/rulerViewTime" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/bottomView"
        android:layout_width="0dp"
        android:layout_height="344dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/panel_bg_line_323f4b"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/timeView">

        <TextView
            android:includeFontPadding="false"
            android:drawablePadding="32dp"
            android:fontFamily="@font/inter_regular"
            android:text="@string/estimated_time"
            android:textSize="44sp"
            android:textColor="@color/white"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="48dp"
            app:drawableStartCompat="@drawable/icon_time_48"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:gravity="center_vertical"
            android:fontFamily="@font/inter_bold"
            android:text=""
            android:textSize="96sp"
            android:textColor="@color/white"
            android:id="@+id/bottomViewEstimatedTime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.constraintlayout.widget.ConstraintLayout>