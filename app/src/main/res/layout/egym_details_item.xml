            <ImageView
                android:id="@+id/ivCoach"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:layout_marginTop="56dp"
                android:importantForAccessibility="no"
                app:layout_constraintStart_toStartOf="@+id/ivPlanIcon"
